<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.web.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/web-project/3">
            <name>ASSIGNMENT_FILTER</name>
            <minimum-ant-version>1.6.5</minimum-ant-version>
            <web-module-libraries>
                <library dirs="200">
                    <file>${libs.jstl.classpath}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library dirs="200">
                    <file>${libs.MySQLDriver.classpath}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
            </web-module-libraries>
            <web-module-additional-libraries/>
            <source-roots>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
        <spellchecker-wordlist xmlns="http://www.netbeans.org/ns/spellchecker-wordlist/1">
            <word>addCart</word>
            <word>AddToCartServlet</word>
            <word>buyBook</word>
            <word>BuyBookServlet</word>
            <word>CheckOutServlet</word>
            <word>CookieServlet</word>
            <word>createNewAccount</word>
            <word>createNewAccountHTML</word>
            <word>createNewAccountJSP</word>
            <word>CreateNewAccountServlet</word>
            <word>DeleteServlet</word>
            <word>errorHTML</word>
            <word>html</word>
            <word>invalidHTML</word>
            <word>jsp</word>
            <word>loadBook</word>
            <word>LoadBookServlet</word>
            <word>loginHTML</word>
            <word>LoginServlet</word>
            <word>LogoutServlet</word>
            <word>removeBook</word>
            <word>RemoveBookServlet</word>
            <word>searchJSP</word>
            <word>SearchServlet</word>
            <word>TGLB</word>
            <word>UpdateServlet</word>
            <word>viewCart</word>
            <word>ViewCartServlet</word>
        </spellchecker-wordlist>
    </configuration>
</project>
